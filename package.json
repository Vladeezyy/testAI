{
  "name": "testai",
  "version": "1.0.0",
  "description": "Playwright automation tests for PICMG BoardBot",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:all": "playwright test tests/",
    "test:advancedmc": "playwright test tests/AdvancedMC/",
    "test:advancedtca": "playwright test tests/AdvancedTCA/",
    "test:microtca": "playwright test tests/MicroTCA/",
    "test:compactpci": "playwright test tests/CompactPCI/",
    "test:comhpc": "playwright test tests/COM-HPC/",
    "test:comexpress": "playwright test tests/COMExpress/",
    "test:suite1": "playwright test --grep TC1\\.",
    "test:suite2": "playwright test --grep TC2\\.",
    "test:suite3": "playwright test --grep TC3\\.",
    "test:ai": "node scripts/run-ai-tests.js",
    "test:ai:advancedmc": "node scripts/run-ai-tests.js tests/AdvancedMC/",
    "test:ai:advancedtca": "node scripts/run-ai-tests.js tests/AdvancedTCA/",
    "test:ai:microtca": "node scripts/run-ai-tests.js tests/MicroTCA/",
    "test:ai:compactpci": "node scripts/run-ai-tests.js tests/CompactPCI/",
    "test:ai:comhpc": "node scripts/run-ai-tests.js tests/COM-HPC/",
    "test:ai:comexpress": "node scripts/run-ai-tests.js tests/COMExpress/",
    "report": "playwright show-report",
    "allure:generate": "allure generate allure-results --clean -o allure-report",
    "allure:open": "allure open allure-report",
    "allure:serve": "allure serve allure-results",
    "allure:report": "npm run allure:generate && npm run allure:open",
    "clean": "node -e \"const fs=require('fs');['test-results','allure-results','allure-report'].forEach(d=>{if(fs.existsSync(d))fs.rmSync(d,{recursive:true,force:true})});console.log('✅ Cleaned all reports')\"",
    "clean:allure": "node -e \"const fs=require('fs');['allure-results','allure-report'].forEach(d=>{if(fs.existsSync(d))fs.rmSync(d,{recursive:true,force:true})});console.log('✅ Cleaned Allure reports')\"",
    "clean:test-results": "node -e \"const fs=require('fs');if(fs.existsSync('test-results'))fs.rmSync('test-results',{recursive:true,force:true});console.log('✅ Cleaned test results')\"",
    "ci": "npm run clean && playwright test --reporter=list,allure-playwright",
    "ci:ai": "npm run clean && node -e \"process.env.USE_AI_VALIDATION='true'\" && playwright test --reporter=list,allure-playwright",
    "update:fixtures": "node utils/updateTestFixtures.js",
    "install:browsers": "playwright install chromium",
    "install:all": "npm install && playwright install chromium"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vladeezyy/testAI.git"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "e2e"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Vladeezyy/testAI/issues"
  },
  "homepage": "https://github.com/Vladeezyy/testAI#readme",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.1",
    "allure-commandline": "^2.32.1",
    "allure-playwright": "^3.0.0",
    "cross-env": "^10.1.0"
  }
}
