{
  "name": "testai",
  "version": "1.0.0",
  "description": "Playwright automation tests for PICMG BoardBot",
  "main": "index.js",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "report": "playwright show-report",
    "report:allure": "allure generate allure-results --clean -o allure-report && allure open allure-report",
    "report:allure:generate": "allure generate allure-results --clean -o docs",
    "report:allure:open": "allure open allure-report",
    "clear:reports": "node utils/clearReports.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vladeezyy/testAI.git"
  },
  "keywords": [
    "playwright",
    "automation",
    "testing",
    "e2e"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/Vladeezyy/testAI/issues"
  },
  "homepage": "https://github.com/Vladeezyy/testAI#readme",
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^24.10.1",
    "allure-commandline": "^2.32.1",
    "allure-playwright": "^3.0.0"
  }
}
